import csv



with open('guided_least.csv', 'rb') as csvfile:
     spamreader = csv.reader(csvfile)
     url = open('url.csv','w')
     for row in spamreader:
              url.write(row[0]+',')
              url.write(row[1]+',')
              url.write(row[2]+',')
              url.write("/"+row[1]+row[2])
               url.write(".f")
              url.write('\n')
              print "/"+row[1]+row[2]+'.f'+','





